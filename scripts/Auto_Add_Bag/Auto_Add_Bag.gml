function Auto_Add_Bag(bag){
	var placed_our_bag=0;
		if Dev_Help[? dev_help.auto_book_in]
			with(oDoorway)
			{
				if placed_our_bag=0
			if Rooms_Type_Map[? my_room]=room_type.bedroom && has_bag=0 
			{
			//	Dev_Help[? dev_help.auto_book_in]=0



						if bag.placed_inside=0
						{
							placed_our_bag=1
							//sm(st(bag)+" "+st(my_room))
								has_bag=1
								bag.room_type_im_in=room_type.bedroom
								bag.room_im_in=my_room
								bag.x=x
								bag.y=y
								bag.floor_i_am_on=floor_i_am_on
						}
				
			
			}
		}
}